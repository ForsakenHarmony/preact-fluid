{
	"extends": ["prettier", "synacor"],
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"comma-dangle": [
			"error",
			"always-multiline"
		],
		"strict": "off",
		// prettier
		"indent": "off",
		"lines-around-comment": "off"
	}
}
