{
	"name": "preact-fluid",
	"version": "0.5.7",
	"description": "",
	"main": "src/index.js",
	"scripts": {
		"start": "npm run styleguide",
		"generate-component": "plop component",
		"lint": "eslint src",
		"test": "npm run prettier-check && preact test",
		"gh-pages": "./node_modules/.bin/gh-pages -d styleguide",
		"styleguide": "styleguidist server",
		"styleguide-build": "cross-env NODE_ENV=production styleguidist build",
		"precommit": "lint-staged",
		"prettier": "prettier --write **/*.js",
		"prettier-check": "prettier --list-different src/**/*.js",
		"publish-docs": "npm run styleguide-build && npm run gh-pages",
		"publish-patch": "npm version patch && npm publish",
		"publish-minor": "npm version minor && npm publish",
		"publish-major": "npm version major && npm publish"
	},
	"lint-staged": {
		"*.{js}": [
			"prettier --write **/*.js",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ajainvivek/preact-fluid.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-module-resolver": "^2.7.1",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"cross-env": "^5.1.3",
		"eslint": "^4.2.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-config-synacor": "^1.0.1",
		"gh-pages": "^1.0.0",
		"husky": "^0.14.3",
		"if-env": "^1.0.0",
		"lint-staged": "^6.0.0",
		"plop": "^1.9.0",
		"preact": "^8.2.0",
		"preact-compat": "^3.16.0",
		"prettier": "1.9.1",
		"prop-types": "^15.5.10",
		"react-styleguidist": "^6.0.21",
		"webpack": "^3.5.5"
	},
	"dependencies": {
		"lodash": "^4.17.4",
